@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityUser>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Users</h2>

<table class="table">
    <tr>
        <th>User Name</th>
        <th></th>
        <th></th>
    </tr>
    <tbody>
        @foreach (var user in this.Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>Manage User Roles</td>
                <td>
                    @if (user.UserName != User.Identity.Name)
                    {
                        @Html.ActionLink("Disable", "Disable", new { id = user.Id })
                    }
                </td>
            </tr>
        }
    </tbody>
</table>